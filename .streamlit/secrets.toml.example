# Streamlit Secrets Configuration (Deployment)
# Copy this file to .streamlit/secrets.toml and fill in your values

# Google Gemini API Key
GEMINI_API = "your-gemini-api-key-here"

# Google Sheet URL (for project matching feature)
GSHEET_URL = "https://docs.google.com/spreadsheets/d/YOUR_SHEET_ID/edit"

# Google Apps Script Web App URL (for document generation)
APPS_SCRIPT_WEB_APP_URL = "https://script.google.com/macros/s/YOUR_DEPLOYMENT_ID/exec"

# Google Sheets Connection Configuration
[connections.gsheets]
spreadsheet = "https://docs.google.com/spreadsheets/d/YOUR_SHEET_ID/edit"
type = "service_account"
# Add your Google Service Account JSON here (from Google Cloud Console)
# See: https://docs.streamlit.io/develop/tutorials/databases/gsheets
